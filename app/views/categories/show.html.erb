<h2>This is category's show page.</h2>
<p>
  <h2><%= @category.name %></h2>
</p>

<%= link_to 'Add new Sub_category', new_category_sub_category_path(@category) %>

<table>
    <% @category.sub_categories.each do |sub_category| %>
        <tr>
        <td><%= sub_category.name %></td>
        </tr>
    <% end %>
</table>